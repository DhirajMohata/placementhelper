<div class="explore" style="display: none;">
    <div class="course-box">
      <ul style="font-weight: bolder;">
        <li id="progressbtn">In progress</li>
        <li id="exploreBtn" class="active">Explore</li>
        <li id="incomingbtn">Incoming</li>
        <li id="eligiblebtn">Eligible</li>
      </ul>
      <% for(var i=0; i<companies.length; i++){ %>
          <% var flag = 0; %>
          <% var flag1 = 0; %>
          <% var flag2 = 0; %>
          <% var flag3 = 0; %>
          <% for(var j=0; j<cStudent.length; j++){ %>
            <% if(companies[i].name === cStudent[j].name) { %>
              <% flag = 1; %>
          <% } %>
          <% } %>
          <% for(var j=0; j<rejected.length; j++){ %>
            <% if(companies[i].name === rejected[j].companyName) { %>
            <% flag1 = 1; %>
            <% } %>
          <% } %>

          <% for(var j=0; j<selected.length; j++){ %>
            <% if(companies[i].name === selected[j].companyName) { %>
            <% flag2 = 1; %>
            <% } %>
          <% } %>
          <% for(var j=0; j<rejectedCompanies.length; j++){ %>
            <% if(companies[i].name === rejectedCompanies[j].companyName) { %>
            <% flag3 = 1; %>
            <% } %>
          <% } %>
          <% var package = student[0].package*0.3 + student[0].package%>
          <% if(flag === 0 && flag1 === 0 && flag2 === 0 && flag3 === 0 && companies[i].package >= package) { %>
            <div class="course">
              <div class="box">
                <h3 style="font-size: 31px";><%=companies[i].name %></h3>
                <p style="color: black;"><%= companies[i].description %></p>
                <br>
                <strong >Role: Software Engineer</strong><br>
                <strong >Package: <%= companies[i].package %></strong><br>
                <form action="/applied/<%= companies[i].email %>" method="post">
                  <button type="submit">Apply</button>
                </form>
              </div>
            </div>
          <% } else if(flag === 0 && flag1 === 0 && flag2 === 0 && flag3 === 0){ %>
            <div class="course">
              <div class="box">
                <h3 style="font-size: 31px";><%=companies[i].name %></h3>
                <p style="color: black;"><%= companies[i].description %></p>
                <br>
                <strong >Role: Software Engineer</strong><br>
                <strong >Package: <%= companies[i].package %></strong><br>
                  <button type="submit" style="background-color: rgb(174, 21, 21);">Can,t apply as company is less then 30% increment</button>
              </div>
            </div>
          <% } %>

        <% } %>
      <% for(var i=0; i<companies.length; i++){ %>
        <% var flag1 = 0; %>
        <% for(var j=0; j<sCompany.length; j++){ %>
          <% if(companies[i].name === sCompany[j]) { %>
          <% flag1 = 1; %>
        <% } %>
      <% } %>
      <% if(flag1 === 1) { %>
      <div class="course">
        <div class="box">
          <h3 style="font-size: 31px";><%=companies[i].name %></h3>
          <p style="color: black;"><%= companies[i].description %></p>
          <br>
          <strong >Role: Software Engineer</strong><br>
          <strong >Package: <%= companies[i].package %></strong><br>
          <button type="submit" style="background-color: rgb(174, 21, 21);">Rejected</button>
        </div>
      </div>
      <% } %>
      <% } %>
      <% for(var i=0; i<companies.length; i++){ %>
      <% var flag = 0; %>
      <% for(var j=0; j<cStudent.length; j++){ %>
      <% if(companies[i].name === cStudent[j].name) { %>
      <% flag = 1; %>
      <% } %>
      <% } %>
      <% if(flag === 1) { %>
      <div class="course">
        <div class="box">
          <h3 style="font-size: 31px";><%=companies[i].name %></h3>
          <p style="color: black;"><%= companies[i].description %></p>
          <br>
          <strong >Role: Software Engineer</strong><br>
          <strong >Package: <%= companies[i].package %></strong><br>
          <button style="background-color: blue;">Already Applied</button>
        </div>
      </div>
      <% } %>
      <% } %>
      <% for(var i=0; i<companiesNot.length; i++){ %>
      <div class="course">
        <div class="box">
          <h3 style="font-size: 31px";><%=companiesNot[i].name %></h3>
          <p style="color: black;"><%= companiesNot[i].description %></p>
          <br>
          <strong >Role: Software Engineer</strong><br>
          <strong >Package: <%= companiesNot[i].package %></strong><br>
          <button style="background-color: rgb(38, 255, 0);">Not Eligible</button>
        </div>
      </div>
      <% } %>
      <% for(var i=0; i<companyPast.length; i++){ %>
      <div class="course">
        <div class="box">
          <h3 style="font-size: 31px";><%= companyPast[i].name %></h3>
          <p style="color: black;"><%= companyPast[i].description %></p>
          <br>
          <strong >Role: Software Engineer</strong><br>
          <strong >Package: <%= companyPast[i].package %></strong><br>
          <button style="background-color: blue;">Not Available Now</button>
        </div>
      </div>
      <% } %>
    </div>
  </div>